{% extends '_layouts/_main.nj' %}

{% block main %}

{% macro GeneratorsCategoryCard(categoryPagePath) %}
  {% set e = getPage(categoryPagePath).props %}
  {% set gens = selectGenerators(data) %}
  {% set eGens = filterWithBrands(gens, e.generatorsBrands) %}
  {% set eGens = filterWithTags(eGens, e.generatorsTags) %}

  {% call components.CategoryCard(
    title = e.contentTitle,
    url = e.url,
    image = '/' + path.images + '/' + e.image,
    power = {
      from: getGeneratorHighestPower(getLeastPowerfulGenerator(eGens)),
      to: getGeneratorHighestPower(getMostPowerfulGenerator(eGens)),
      units: ' кВт'
    },
    discount = getGeneratorsWithHighestDiscount(eGens).discount
  ) %}
    {{ e.shortDesc|safe }}
  {% endcall %}
{% endmacro %}

